[flake8]
ignore =
    C103,
    C111,
    C301,
    C901,
    C1001,
    E501,
    E1101,
    E1102,
    E1103,
    R201,
    R901,
    R903,
    R904,
    R801,
    W142,
    W201,
    W223,
    W232,
    W402
max-line-length = 120
exclude =
    .git,
    __pycache__,
    launch,
    msg
max-complexity = 10
disable-noqa = False
show_source = True
statistics = True
